Options +FollowSymlinks
RewriteEngine on

#---------------------------------------------------------------------------------------------------------------------------
# SYSTEM
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^signin/load$ 			?cmd=SigninLoad [L]
RewriteRule ^signin/exe$ 			?cmd=SigninExe [L]
RewriteRule ^signout/load$ 			?cmd=SignoutLoad [L]
RewriteRule ^signout/exe$ 			?cmd=SignoutExe [L]
RewriteRule ^change/pass/load$ 		?cmd=ChangePassLoad [L]
RewriteRule ^change/pass/exe$ 		?cmd=ChangePassExe [L]

RewriteRule ^home$ 					?cmd=Home [L]

RewriteRule ^contact/ins/exe$ 		?cmd=ContactInsExe [L]

#---------------------------------------------------------------------------------------------------------------------------
# SETTING
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^setting$ 												?cmd=Setting [L]

#---------------------------------------------------------------------------------------------------------------------------
# SETTING.CATEGORY.NEWS
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^setting/category/news$ 								?cmd=SettingCategoryNews [L]
RewriteRule ^setting/category/news/page/([0-9])$ 					?cmd=SettingCategoryNews&Page=$1 [L]

RewriteRule ^setting/category/news/ins/load$ 						?cmd=SettingCategoryNewsInsLoad [L]
RewriteRule ^setting/category/news/ins/exe$ 						?cmd=SettingCategoryNewsInsExe [L]
RewriteRule ^setting/category/news/([0-9])/upd/load$ 				?cmd=SettingCategoryNewsUpdLoad&IdCategory=$1 [L]
RewriteRule ^setting/category/news/([0-9])/upd/exe$ 				?cmd=SettingCategoryNewsUpdExe&IdCategory=$1 [L]
RewriteRule ^setting/category/news/([0-9])/del/load$ 				?cmd=SettingCategoryNewsDelLoad&IdCategory=$1 [L]
RewriteRule ^setting/category/news/([0-9])/del/exe$ 				?cmd=SettingCategoryNewsDelExe&IdCategory=$1 [L]

#---------------------------------------------------------------------------------------------------------------------------
# SETTING.CATEGORY.ALBUM
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^setting/category/album$ 								?cmd=SettingCategoryAlbum [L]
RewriteRule ^setting/category/album/page/([0-9])$ 					?cmd=SettingCategoryAlbum&Page=$1 [L]

RewriteRule ^setting/category/album/ins/load$ 						?cmd=SettingCategoryAlbumInsLoad [L]
RewriteRule ^setting/category/album/ins/exe$ 						?cmd=SettingCategoryAlbumInsExe [L]
RewriteRule ^setting/category/album/([0-9])/upd/load$ 				?cmd=SettingCategoryAlbumUpdLoad&IdCategory=$1 [L]
RewriteRule ^setting/category/album/([0-9])/upd/exe$ 				?cmd=SettingCategoryAlbumUpdExe&IdCategory=$1 [L]
RewriteRule ^setting/category/album/([0-9])/del/load$ 				?cmd=SettingCategoryAlbumDelLoad&IdCategory=$1 [L]
RewriteRule ^setting/category/album/([0-9])/del/exe$ 				?cmd=SettingCategoryAlbumDelExe&IdCategory=$1 [L]

#---------------------------------------------------------------------------------------------------------------------------
# SETTING.CATEGORY.VIDEO
#---------------------------------------------------------------------------------------------------------------------------
RewriteRule ^setting/category/video$ 								?cmd=SettingCategoryVideo [L]
RewriteRule ^setting/category/video/page/([0-9])$ 					?cmd=SettingCategoryVideo&Page=$1 [L]

RewriteRule ^setting/category/video/ins/load$ 						?cmd=SettingCategoryVideoInsLoad [L]
RewriteRule ^setting/category/video/ins/exe$ 						?cmd=SettingCategoryVideoInsExe [L]
RewriteRule ^setting/category/video/([0-9])/upd/load$ 				?cmd=SettingCategoryVideoUpdLoad&IdCategory=$1 [L]
RewriteRule ^setting/category/video/([0-9])/upd/exe$ 				?cmd=SettingCategoryVideoUpdExe&IdCategory=$1 [L]
RewriteRule ^setting/category/video/([0-9])/del/load$ 				?cmd=SettingCategoryVideoDelLoad&IdCategory=$1 [L]
RewriteRule ^setting/category/video/([0-9])/del/exe$ 				?cmd=SettingCategoryVideoDelExe&IdCategory=$1 [L]